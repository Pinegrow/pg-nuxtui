    "pinia": "^2.1.6"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.96",
    "@nuxt/content": "^2.8.2",
    "@nuxt/devtools": "^0.7.1",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/image": "^1.0.0",
    "@nuxt/ui": "^2.9.0",
    "@nuxtjs/critters": "^0.5.1",
