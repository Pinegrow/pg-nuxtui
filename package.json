    "@pinegrow/nuxtui-plugin": "^3.0.15",
    "@pinegrow/tailwindcss-plugin": "^3.0.5",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
