    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.5.0",
    "@unocss/nuxt": "^0.54.1",
    "@unocss/preset-icons": "^0.54.1",
    "@vue/devtools": "^6.5.0",
